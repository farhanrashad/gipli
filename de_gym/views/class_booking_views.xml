<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
        
        <record id="class_booking_tree_view" model="ir.ui.view">
			<field name="name">class.booking.tree.view</field>
			<field name="model">gym.class.booking</field>
			<field name="arch" type="xml">
				<tree string="Class Booking" editable="bottom">
                    <field name="class_planning_id" column_invisible="1"/>
					<field name="member_id"/>
                    <field name="class_type_id" readonly="1"/>
				</tree>
			</field>
		</record>
        <record id="class_booking_search_view" model="ir.ui.view">
            <field name="name">class.booking.search.view</field>
            <field name="model">gym.class.booking</field>
            <field name="arch" type="xml">
                <search string="Class Booking">
                    <field name="member_id" />
                </search>
             </field>
        </record>
        
        <record id="action_class_booking" model="ir.actions.act_window">
            <field name="name">Class Booking</field>
            <field name="res_model">gym.class.booking</field>
            <field name="view_mode">tree</field>
            <field name="search_view_id" ref="class_booking_search_view"/>
            <field name="context">{
                
                }
            </field>
        </record>
    </data>
</odoo>