<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="const_type_na" model="vote.const.type">
            <field name="name">National Assembly Constituencies</field>
        </record>

        <record id="const_type_pa" model="vote.const.type">
            <field name="name">Provincial Assembly Constituencies</field>
        </record>

        <record id="const_type_senate" model="vote.const.type">
            <field name="name">Senate Constituencies</field>
        </record>

        <record id="const_type_lb" model="vote.const.type">
            <field name="name">Local Government Constituencies</field>
        </record>

        <record id="const_type_tribal" model="vote.const.type">
            <field name="name">Tribal Areas Constituencies</field>
        </record>

        <record id="const_type_women" model="vote.const.type">
            <field name="name">Women and Minority Reserved Constituencies</field>
        </record>


        <!-- Member References Type -->
        <record id="member_ref_type_proposer" model="vote.member.ref.type">
            <field name="name">Proposer</field>
        </record>

        <record id="member_ref_type_seconder" model="vote.member.ref.type">
            <field name="name">Seconder</field>
        </record>
        
        <!-- Election Data -->
        <record id="elect_stage_0" model="vote.elect.stage">
            <field name="sequence">10</field>
            <field name="name">Inquiry</field>
        </record>
    
        <record id="elect_stage_1" model="vote.elect.stage">
            <field name="sequence">15</field>
            <field name="name">Application Recived</field>
        </record>
    
        <record id="elect_stage_2" model="vote.elect.stage">
            <field name="sequence">20</field>
            <field name="name">Review</field>
        </record>
    
        <record id="elect_stage_3" model="vote.elect.stage">
            <field name="sequence">25</field>
            <field name="name">Acceptance</field>
            <field name="fold" eval="True"/>
        </record>
    
        

        
    </data>
</odoo>
