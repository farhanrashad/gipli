<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="action_elect_member" model="ir.actions.server">
        <field name="name">Members Pipeline</field>
        <field name="model_id" ref="de_vote.model_vote_elect_year"/>
        <field name="state">code</field>
        <field name="groups_id"  eval="[(4, ref('base.group_user'))]"/>
        <field name="code">action = model.action_elect_member()</field>
    </record>

    <record id="action_admission_forecast" model="ir.actions.server">
        <field name="name">Admission: Forecast</field>
        <field name="model_id" ref="de_vote.model_vote_elect_year"/>
        <field name="state">code</field>
        <field name="groups_id"  eval="[(4, ref('base.group_user'))]"/>
        <field name="code">action = model.action_admission_forecast()</field>
    </record>
    
</odoo>
