<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="team_admission_pioneers" model="oe.admission.team">
            <field name="name">Admission Pioneers</field>
            <field name="sequence">0</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="admission_team_member_admin_admission" model="oe.admission.team.member" forcecreate="0">
            <field name="admission_team_id" ref="team_admission_pioneers"/>
            <field name="user_id" ref="base.user_admin"/>
        </record>

        <record id="team_horizon_seekers" model="oe.admission.team">
            <field name="name">Horizon Seekers</field>
            <field name="company_id" eval="False"/>
            <field name="active" eval="False"/>
        </record>

        <function model="oe.admission.team" name="message_unsubscribe"
            eval="[
            ref('de_school_team.team_admission_pioneers'),
            ref('de_school_team.team_horizon_seekers')],
            [ref('base.partner_root')]"/>
    </data>
</odoo>
