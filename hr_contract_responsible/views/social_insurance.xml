<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="social_insurance_form_view" model="ir.ui.view">
            <field name="name">social.insurance.form</field>
            <field name="model">social.insurance</field>
            <field name="arch" type="xml">
                <form string="Social Insurance">
                    <sheet>

                        <group>
                            <group>
                                <field name="social_insurance_package"/>
                            </group>
                            <group>
                                <field name="employee_percentage"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="social_insurance_tree_view" model="ir.ui.view">
            <field name="name">social.insurance.tree</field>
            <field name="model">social.insurance</field>
            <field name="arch" type="xml">
                <tree string="Social Insurance">
                    <field name="social_insurance_package"/>
                    <field name="employee_percentage"/>
                </tree>
            </field>
        </record>


        <record id="social_insurance_act_window" model="ir.actions.act_window">
            <field name="name">Social Insurance</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">social.insurance</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    There is no examples click here to add new Social Insurance.
                </p>
            </field>
        </record>


    </data>
</odoo>