<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="base_partner_merge_automatic_wizard_form" model="ir.ui.view">
        <field name="model">base.partner.merge.automatic.wizard</field>
        <field name="inherit_id" ref="base.base_partner_merge_automatic_wizard_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='dst_partner_id']" position="after">
                <field name="dst_partner_id_id"/>

            </xpath>
            <xpath
                expr="//field[@name='partner_ids']/tree//field[@name='country_id']"
                position="after"
            >
                     <field name="category_id" widget="many2many_tags"/>
            </xpath>
        </field>
    </record>
</odoo>
