<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        
        <record id="real_partner_ledger_view_tree" model="ir.ui.view">
            <field name="name">real_partner_ledger_view_tree</field>
            <field name="model">account.payment</field>
            <field name="arch" type="xml">
                <tree string="real_partner_ledger_tree" create="0" edit="0" delete="0">
                    <field name="property_id" string="Unit Code"/>
                    <field name="partner_id" string="Customer  Name"/>
                    <field name="journal_id" string="Payment  Method"/>
                    <field name="state" string="توجيه"/>
                    <field name="check_number"/>
                    <field name="due_date"/>
                    <field name="description23"/>
                    <field name="amount"/>
                </tree>
            </field>
        </record>
        <record id="real_partner_ledger_action" model="ir.actions.act_window">
            <field name="name">Real Estate Partner Ledger</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">account.payment</field>
            <field name="view_mode">tree</field>
            <field name="domain">[('reservation_id','!=',False)]</field>
            <field name="view_id" ref="add_real_estate.real_partner_ledger_view_tree"/>
        </record>
        <!-- This Menu Item must have a parent and an action -->
        <menuitem id="real_partner_ledger_menu" name="Real Estate Partner Ledger" parent="add_real_estate.menu_real_estate" action="real_partner_ledger_action" sequence="300"/>
        <menuitem id="real_partner_ledger_menu2" name="Real Estate Partner Ledger" parent="account_reports.account_reports_partners_reports_menu" action="real_partner_ledger_action" sequence="300"/>

        
    
    </data>
</odoo>