            <field name="model">notify.disc</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">add_real_estate.report_notify_discount</field>
            <field name="report_file">add_real_estate.report_notify_discount</field>
            <field name="binding_model_id" ref="add_real_estate.model_notify_disc"/>
            <field name="binding_type">report</field>
        </record>

</odoo>