            </xpath> -->
        </template>

        <template id="portal_my_home_report" name="Show Types / Reports" customize_show="True" inherit_id="portal.portal_my_home" priority="50">
            <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
                <t t-call="portal.portal_docs_entry">
                    <t t-set="title">Reports</t>
                    <t t-set="url" t-value="'/my/reports'"/>
                    <t t-set="placeholder_count" t-value="'report_count'"/>
                </t>
            </xpath>
        </template>

        <!-- ======================================== -->
        <!-- ===== Portal menu Items Template ======= -->
        <!-- ======================================== -->

        <template id="portal_my_hr_report_list" name="My Self Reports">
            <t t-call="portal.portal_layout">
                <t t-set="breadcrumbs_searchbar" t-value="True"/>
                <t t-call="portal.portal_searchbar">
                    <t t-set="title">Reports</t>
                </t>
                <t t-if="not reports">
                    <div class="alert alert-warning mt8" role="alert">
                    There are no reports.
                    </div>
                </t>
                <t t-if="reports" t-call="portal.portal_table">
                    <tbody>
                        <tr t-foreach="reports" t-as="report">

                            <t t-set="group" t-value="report.group_id"/>
                            <t t-if="group  &amp;  request.env.user.groups_id">
                                <td>
                                    <a t-attf-href="/my/report/{{report.id}}">
                                        <span t-field="report.name"/>
                                    </a>
                                    <br/>
                                </td>

                            </t>

                            <t t-if="not group">
                                <td>
                                    <a t-attf-href="/my/report/{{report.id}}">
                                        <span t-field="report.name"/>
                                    </a>
                                    <br/>
                                </td>

                            </t>








                            <!-- <td class="text-right"> -->
                            <!-- <span class="badge badge-secondary badge-pill" data-oe-model="ir.ui.view" data-oe-id="report.get_record_count( user_id)" data-oe-field="arch" data-placeholder_count="report.get_record_count(user_id)"><t t-esc="report.get_record_count(user_id)" /></span> -->
