<odoo>
    <data>
        <template id="portal_layout_report" name="Portal layout: project menu entry" inherit_id="portal.portal_breadcrumbs" priority="50">
            <xpath expr="//ol[hasclass('o_portal_submenu')]" position="inside">
                <li t-if="page_name == 'report' or report" class="col-lg-2" t-attf-class="breadcrumb-item #{'active ' if not report else ''}">
                    <a t-if="report" t-attf-href="/my/reports?{{ keep_query() }}">Reports</a>
                    <t t-else="">Reports</t>
                </li>
                <li t-if="report" class="breadcrumb-item active text-truncate col-8 col-lg-10">
                    <a t-if="report" t-attf-href="/my/report/{{report.id}}">
                        <span t-esc="report.model_id.name" />
                    </a>
                    <t t-else="">
                        <span t-esc="report.model_id.name" />
                    </t>
                </li>
            </xpath>
            <!-- <xpath expr="//ol[hasclass('o_portal_submenu')]" position="inside">
