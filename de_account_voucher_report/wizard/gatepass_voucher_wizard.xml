<?xml version="1.0" encoding="utf-8"?>
<odoo> 
        <!--Display form view of wizard-->    
        <record id="view_gatepass_voucher_wizard_form" model="ir.ui.view">
            <field name="name">gatepass.voucher.wizard.form</field>
            <field name="model">gatepass.voucher.wizard</field>
            <field name="arch" type="xml">
                <form string="GatePass Voucher">
                    <group>
                      <group>
                        </group>
                      <group>
                          <field name="start_date" />
                        <field name="end_date" />
                      </group>
                        
                    </group>
                    <footer>
                        <button string='Print' name="check_report" type="object" default_focus="1" class="oe_highlight"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <!--Open the wizard on click of menu-->
        <record id="open_gatepass_voucher_action" model="ir.actions.act_window">
            <field name="name">Select Date to print </field>
            <field name="res_model">gatepass.voucher.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
            <field name="view_id" ref="view_gatepass_voucher_wizard_form"/>
        </record>


</odoo>