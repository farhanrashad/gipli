<odoo>
    <data>
        <template id="portal_service_record_form" name="My Entries">
            <t t-call="portal.portal_layout">
                <t t-set="breadcrumbs_searchbar" t-value="True"/>
                <form action="/my/model/record/submit" method="post" enctype="multipart/form-data" class="o_mark_required row" data-mark="*" data-success-page="">
                    <input type="hidden" name="csrf_token" value="{{ csrf_token() }}"/>
                    <div class="s_website_form_rows row s_col_no_bgcolor">
                        <div class="form-group s_website_form_field col-12 " data-type="char" data-name="Field">
                            <input type="hidden" class="form-control s_website_form_input" id="service_id" name="service_id" t-att-value="service_id.id" />
                            <input type="hidden" class="form-control s_website_form_input" id="model_id" name="model_id" t-att-value="model_id" />
                            <input type="hidden" class="form-control s_website_form_input" id="record_id" name="record_id" t-att-value="record_id" />
                            <input type="hidden" class="form-control s_website_form_input" id="edit_mode" name="edit_mode" t-att-value="edit_mode" />
                        </div>
                        <t t-raw="template_primary_fields"/>
                        <!--
                            <div class="form-group col-12 s_website_form_submit" data-name="Submit Button" style="text-align: right;">
                                <div style="width: 200px;" class="s_website_form_label"/>
                                <button type="submit" class="btn btn-primary rounded-circle btn-lg">Submit</button>
                                <span id="s_website_form_result"/>
                            </div>
                        -->
                    </div>
                </form>    
            </t>
        </template>
    </data>
</odoo>