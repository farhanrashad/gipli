    <record model="ir.ui.view" id="sh_retail_view_move_form_inherit">
        <field name="name">sh.retail.view.move.form.inherit</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="sh_retail_price_tax.sh_account_move_form_view"/>
        <field name="arch" type="xml">
      
            <xpath expr="//field[@name='invoice_line_ids']//tree//field[@name='sh_retail_price']" position="replace">
                        <!-- <field name="sh_retail_price"/> -->
            </xpath>
        </field>
    </record>    
    
    
    <record model="ir.ui.view" id="view_move_form_inherit">
        <field name="name">view.move.form.inherit</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
        <xpath expr="/form[1]/sheet[1]/notebook[1]/page[1]/field[@name='invoice_line_ids']/tree[1]/field[@name='quantity']" position="replace">
                  
        </xpath>
        <xpath expr="/form[1]/sheet[1]/notebook[1]/page[1]/field[@name='invoice_line_ids']/tree[1]/field[@name='price_unit']" position="replace">
                  
        </xpath>
        <xpath expr="/form[1]/sheet[1]/notebook[1]/page[1]/field[@name='invoice_line_ids']/tree[1]/field[@name='product_uom_id']" position="replace">
                  
        </xpath>
        <xpath expr="/form[1]/sheet[1]/notebook[1]/page[1]/field[@name='invoice_line_ids']/tree[1]/field[@name='name']" position="replace">
                  
        </xpath>
        
         <xpath expr="/form[1]/sheet[1]/notebook[1]/page[1]/field[@name='invoice_line_ids']/tree[1]/field[@name='tax_ids']" position="replace">
                  
        </xpath>
        
        
        <xpath expr="/form[1]/sheet[1]/notebook[1]/page[1]/field[@name='invoice_line_ids']/tree[1]/field[@name='price_subtotal']" position="replace">
                  
        </xpath>
        
        
         <xpath expr="/form[1]/sheet[1]/notebook[1]/page[1]/field[@name='invoice_line_ids']/tree[1]/field[@name='account_id']" position="replace">
                  
        </xpath>
        <xpath expr="//form[1]/sheet[1]/notebook[1]/page[1]/field[@name='invoice_line_ids']/tree[1]/field[@name='product_id']" position="after">
            <field name="quantity"/>
            <field name="invoiced_qty"/>
            <field name="sh_retail_price"/>
            <field name="price_unit"/>
            
            <field name="product_uom_id"/>
            <field name="name"/>
            <field name="account_id"/>
             <field name="tax_ids" widget="many2many_tags"/>
            <field name="price_subtotal"/>
            
        </xpath>
              
        </field>
    </record>
