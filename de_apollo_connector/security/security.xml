<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Define access control for apl.people.results model -->
    <record id="acl_apl_people_results_create" model="ir.rule">
        <field name="name">No Create Access</field>
        <field name="model_id" ref="model_apl_people_results"/>
        <field name="domain_force">[(1,'=',1)]</field>
        <field name="perm_create" eval="False"/>
        <field name="active" eval="True"/>
    </record>

    <record id="acl_apl_people_results_edit" model="ir.rule">
        <field name="name">No Edit Access</field>
        <field name="model_id" ref="model_apl_people_results"/>
        <field name="domain_force">[(1,'=',1)]</field>
        <field name="perm_write" eval="False"/>
        <field name="active" eval="True"/>
    </record>
</odoo>
