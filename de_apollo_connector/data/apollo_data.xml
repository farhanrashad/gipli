<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!-- Scheduler -->
        <record forcecreate="True" id="ir_cron_contact_send_apollo_action" model="ir.cron">
            <field name="name">Contacts update at Apollo: run scheduler</field>
            <field name="model_id" ref="model_res_partner"/>
            <field name="state">code</field>
            <field name="code">
model._cron_send_to_apollo()
            </field>
            <field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
        </record>

        <!-- Scheduler -->
        <record forcecreate="True" id="ir_cron_loan_scheduler_action" model="ir.cron">
            <field name="name">Opportunities update at Apollo: run scheduler</field>
            <field name="model_id" ref="model_crm_lead"/>
            <field name="state">code</field>
            <field name="code">
model._cron_send_to_apollo()
            </field>
            <field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
        </record>
        
    </data>
</odoo>