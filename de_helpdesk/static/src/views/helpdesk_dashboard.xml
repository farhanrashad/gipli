<?xml version="1.0" encoding="UTF-8"?>
<templates>
    <t t-name="de_helpdesk.HelpdeskDashboard" owl="1">
        <div class="o_helpdesk_dashboard container-fluid py-4 border-bottom bg-white">
            <div class="row">
                <div class="col-6">
                    <table class="o_helpdesk_banner_table table table-sm table-borderless h-100 px-3 mb-0 text-center">
                        <tbody>
                            <tr class="h-100">
                                <td class="col-3 px-1 o_text py-2 text-start align-middle">
                                    <h3 class="o_highlight">My Tickets</h3>
                                    Open Tickets
                                </td>
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" 
                                        name="de_helpdesk.action_project_helpdesk_my_ticket"
                                        class="btn w-100 h-100 p-2 border-0 text-capitalize fw-normal btn-primary"
                                        title="My Tickets"
                                        context="{'search_default_high_priority': True}"
                                        data-hotkey="p" 
                                        t-on-click="onActionClicked"
                                    >
                                        <span class="d-block mb-0 fs-2" t-out="helpdeskData['all_quotation']"/>Tickets
                                    </a>
                                </td>
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" class="btn w-100 h-100 p-2 border-0 text-capitalize fw-normal btn-primary">
                                        <span class="d-block mb-0 fs-2" t-out="helpdeskData['all_quotation']"/>High Priority
                                    </a>
                                </td>
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" class="btn w-100 h-100 p-2 border-0 text-capitalize fw-normal btn-primary">
                                        <span class="d-block mb-0 fs-2" t-out="helpdeskData['all_quotation']"/>Urgent
                                    </a>
                                </td> 
                            </tr>
                            <tr class="h-100">
                                <td class="col-3 px-1 o_text py-2 text-start align-middle">
                                    Avg Open Hours
                                </td>
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" class="btn w-100 p-2 border-0 text-capitalize fw-normal">
                                        
                                            <span class="mb-0 text-reset" t-out="helpdeskData['all_quotation']"/>
                                        
                                    </a>
                                </td> 
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" class="btn w-100 p-2 border-0 text-capitalize fw-normal">
                                            <span class="mb-0 text-reset" t-out="helpdeskData['all_quotation']"/>
                                    </a>
                                </td> 
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" class="btn w-100 p-2 border-0 text-capitalize fw-normal">
                                            <span class="mb-0 text-reset" t-out="helpdeskData['all_quotation']"/>
                                    </a>
                                </td> 
                            </tr>
                            <tr class="h-100">
                                <td class="col-3 px-1 o_text py-2 text-start align-middle">
                                    Failed Tickets
                                </td>
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" class="btn w-100 p-2 border-0 text-capitalize fw-normal">
                                            <span class="mb-0 text-reset" t-out="helpdeskData['all_quotation']"/>
                                    </a>
                                </td> 
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" class="btn w-100 p-2 border-0 text-capitalize fw-normal">
                                            <span class="mb-0 text-reset" t-out="helpdeskData['all_quotation']"/>
                                    </a>
