<?xml version="1.0" encoding="UTF-8"?>
<templates>
    <t t-name="de_helpdesk.HelpdeskDashboard" owl="1">
        <div class="o_helpdesk_dashboard container-fluid py-4 border-bottom bg-white">
            <div class="row">
                <div class="col-6">
                    <table class="o_helpdesk_banner_table table table-sm table-borderless h-100 px-3 mb-0 text-center">
                        <tbody>
                            <tr class="h-100">
                                <td class="col-3 px-1 o_text py-2 text-start align-middle">
                                    <h3 class="o_highlight">My Tickets</h3>
                                    Open Tickets
                                </td>
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" 
                                        name="de_helpdesk.action_dashboard_ticket_all"
                                        class="btn w-100 p-2 border-0 text-capitalize fw-normal btn-primary"
                                        title="My Tickets"
                                        context="{
                                            'search_default_my_ticket': 1,
                                            'search_default_open_tickets':1,
                                            'search_view_ref': 'context_search',
                                        }"
                                        t-on-click="onActionClicked"
                                    >
                                        <span class="d-block mb-0 fs-2" t-out="helpdeskData['count_my_tickets']"/>Tickets
                                    </a>
                                </td>
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" 
                                        name="de_helpdesk.action_dashboard_ticket_all"
                                        class="btn w-100 p-2 border-0 text-capitalize fw-normal btn-primary"
                                        title="My Tickets"
                                        context="{
                                            'search_default_my_ticket': 1,
                                            'search_default_open_tickets':1,
                                            'search_default_priority_high':1,
                                            'search_view_ref': 'context_search',
                                        }"
                                        t-on-click="onActionClicked"
                                    >
                                        <span class="d-block mb-0 fs-2" t-out="helpdeskData['count_my_high_tickets']"/>High Priority
                                    </a>
                                </td>
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" 
                                        name="de_helpdesk.action_dashboard_ticket_all"
                                        class="btn w-100 p-2 border-0 text-capitalize fw-normal btn-primary"
                                        title="My Tickets"
                                        context="{
                                            'search_default_my_ticket': 1,
                                            'search_default_open_tickets':1,
                                            'search_default_priority_urgent':1,
                                            'search_view_ref': 'context_search',
                                        }"
                                        t-on-click="onActionClicked"
                                    >
                                        <span class="d-block mb-0 fs-2" t-out="helpdeskData['count_my_urgent_tickets']"/>Urgent
                                    </a>
                                </td> 
                            </tr>
                            <tr class="h-100">
                                <td class="col-3 px-1 o_text py-2 text-start align-middle">
                                    Avg Open Hours
                                </td>
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" class="btn w-100 p-2 border-0 text-capitalize fw-normal">
                                        
                                            <span class="mb-0 text-reset" t-out="helpdeskData['all_quotation']"/>
                                        
                                    </a>
                                </td> 
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" class="btn w-100 p-2 border-0 text-capitalize fw-normal">
                                            <span class="mb-0 text-reset" t-out="helpdeskData['all_quotation']"/>
                                    </a>
                                </td> 
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" class="btn w-100 p-2 border-0 text-capitalize fw-normal">
                                            <span class="mb-0 text-reset" t-out="helpdeskData['all_quotation']"/>
                                    </a>
                                </td> 
                            </tr>
                            <tr class="h-100">
                                <td class="col-3 px-1 o_text py-2 text-start align-middle">
                                    Failed Tickets
                                </td>
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" 
                                        name="de_helpdesk.action_dashboard_ticket_all"
                                        class="btn w-100 p-2 border-0 text-capitalize fw-normal"
                                        title="My Tickets"
                                        context="{
                                            'search_default_my_ticket': 1,
                                            'search_default_sla_failed':1,
                                            'search_default_open_tickets':1,
                                            'search_view_ref': 'context_search',
                                        }"
                                        t-on-click="onActionClicked"
                                    >
                                            <span class="mb-0 text-reset" t-out="helpdeskData['all_quotation']"/>
                                    </a>
                                </td> 
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" 
                                        name="de_helpdesk.action_dashboard_ticket_all"
                                        class="btn w-100 p-2 border-0 text-capitalize fw-normal"
                                        title="My Tickets"
                                        context="{
                                            'search_default_my_ticket': 1,
                                            'search_default_sla_failed':1,
                                            'search_default_open_tickets':1,
                                            'search_default_priority_high':1,
                                            'search_view_ref': 'context_search',
                                        }"
                                        t-on-click="onActionClicked"
                                    >
                                            <span class="mb-0 text-reset" t-out="helpdeskData['all_quotation']"/>
                                    </a>
                                </td> 
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" 
                                        name="de_helpdesk.action_dashboard_ticket_all"
                                        class="btn w-100 p-2 border-0 text-capitalize fw-normal"
                                        title="My Tickets"
                                        context="{
                                            'search_default_my_ticket': 1,
                                            'search_default_sla_failed':1,
                                            'search_default_open_tickets':1,
                                            'search_default_priority_urgent':1,
                                            'search_view_ref': 'context_search',
                                        }"
                                        t-on-click="onActionClicked"
                                    >
                                            <span class="mb-0 text-reset" t-out="helpdeskData['all_quotation']"/>
                                    </a>
                                </td> 
                            </tr>
                        </tbody>
                    </table>
                </div>
                <div class="col-6">
                    <table class="o_helpdesk_banner_table table table-sm table-borderless h-100 px-3 mb-0 text-center">
                        <tbody>
                            <tr class="h-100">
                                <td class="col-3 px-1 o_text py-2 text-start align-middle">
                                    <h3 class="o_highlight">My Performance</h3>
                                    Today
                                </td>
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" 
                                        name="de_helpdesk.action_dashboard_ticket_all"
                                        class="btn w-100 h-100 p-2 border-0 text-capitalize fw-normal"
                                        title="My Tickets"
                                        context="{
                                            'search_default_my_ticket': 1,
                                            'search_default_close_tickets':1,
                                            'search_view_ref': 'context_search',
                                        }"
                                        t-on-click="onActionClicked"
                                    >
                                        <div class="p-2">
                                            <h2 class="mb-0 text-reset" t-out="helpdeskData['count_my_closed_tickets']"/>
                                            Closed Tickets
                                        </div>
                                    </a>
                                </td>
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" 
                                        name="de_helpdesk.action_dashboard_ticket_all"
                                        class="btn w-100 p-2 border-0 text-capitalize fw-normal"
                                        title="My Tickets"
                                        context="{
                                            'search_default_my_ticket': 1,
                                            'search_default_sla_success':1,
                                            'search_view_ref': 'context_search',
                                        }"
                                        t-on-click="onActionClicked"
                                    >
                                        <div class="p-2">
                                            <h2 class="mb-0 text-reset" t-out="helpdeskData['all_quotation']"/>
                                            SLA Success Rate
                                        </div>
                                    </a>
                                </td>
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" 
                                        name="de_helpdesk.action_dashboard_customer_rating"
                                        class="btn w-100 p-2 border-0 text-capitalize fw-normal"
                                        title="Customer Ratings"
                                        context="{
                                            'search_default_my_ratings': 1,
                                            'search_default_rating_today':1,
                                            'search_view_ref': 'context_search',
                                        }"
                                        t-on-click="onActionClicked"
                                    >
                                        <div class="p-2">
                                            <h2 class="mb-0 text-reset" t-out="helpdeskData['all_quotation']"/>
                                            Average Rating
                                        </div>
                                    </a>
                                </td> 
                            </tr>
                            <tr class="h-100">
                                <td class="col-3 px-1 o_text py-2 text-start align-middle">
                                    Avg Last 7 Days
                                </td>
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" class="btn w-100 p-2 border-0 text-capitalize fw-normal btn-warning">
                                            <span class="mb-0 text-reset" t-out="helpdeskData['all_quotation']"/>
                                    </a>
                                </td> 
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" class="btn w-100 p-2 border-0 text-capitalize fw-normal btn-warning">
                                            <span class="mb-0 text-reset" t-out="helpdeskData['all_quotation']"/>
                                    </a>
                                </td> 
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" 
                                        name="de_helpdesk.action_dashboard_customer_rating"
                                        class="btn w-100 p-2 border-0 text-capitalize fw-normal"
                                        title="Customer Ratings"
                                        context="{
                                            'search_default_my_ratings': 1,
                                            'search_default_rating_last_7_days':1,
                                            'search_view_ref': 'context_search',
                                        }"
                                        t-on-click="onActionClicked"
                                    >
                                            <span class="mb-0 text-reset" t-out="helpdeskData['all_quotation']"/>
                                    </a>
                                </td> 
                                
                            </tr>
                            <tr class="h-100">
                                <td class="col-3 px-1 o_text py-2 text-start align-middle">
                                    Daily Target
                                </td>
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" class="btn w-100 p-2 border-0 text-capitalize fw-normal">
                                            <span class="mb-0 text-reset" t-out="helpdeskData['all_quotation']"/>
                                    </a>
                                </td> 
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" class="btn w-100 p-2 border-0 text-capitalize fw-normal">
                                            <span class="mb-0 text-reset" t-out="helpdeskData['all_quotation']"/>
                                    </a>
                                </td> 
                                <td t-attf-class="col-3 px-1">
                                    <a href="#" class="btn w-100 p-2 border-0 text-capitalize fw-normal">
                                            <span class="mb-0 text-reset" t-out="helpdeskData['all_quotation']"/>
                                    </a>
                                </td> 
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </t>
</templates>
