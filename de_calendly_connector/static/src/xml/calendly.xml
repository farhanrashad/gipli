<?xml version="1.0" encoding="UTF-8"?>
<templates>
    <t t-name="de_calendly_connector.GoogleCalendarController" t-inherit="calendar.AttendeeCalendarController" t-inherit-mode="extension">
        <FilterPanel position="after">
            <div id="calendly_calendar_sync" class="o_calendar_sync" t-if="model.googleCredentialsSet">
                <!-- New button for Calendly -->
                <button type="button" id="calendly_button" class="o_calendly_button btn btn-primary text-nowrap" t-on-click="onCalendlyButtonClick">
                    <b><i class='fa fa-calendar'/> Calendly</b>
                </button>


            </div>
        </FilterPanel>
    </t>
</templates>
