<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        
        <!-- Define the tree view for Calendly event type -->
        <record id="calendly_event_type_tree_view" model="ir.ui.view">
            <field name="name">calendly.event.type.tree.view</field>
            <field name="model">calendly.event.type</field>
            <field name="arch" type="xml">
                <tree string="Calendly Event Type">
                    <field name="name"/>
                    <field name="uri"/>
                </tree>
            </field>
        </record>

        <!-- Define the action for the Calendly event type -->
        <record id="action_calendly_even_type" model="ir.actions.act_window">
            <field name="name">Calendly Event Type</field>
            <field name="res_model">calendly.event.type</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem id="calendar_menu_config_calendly_even_type" name="Event Type" parent="calendar_menu_config_calendly" action="action_calendly_even_type" sequence="20"/>

    </data>
</odoo>
