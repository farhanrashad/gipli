<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="ntn_partner_detail_account_move_form_view" model="ir.ui.view">
			<field name="name">detail.account.move.line.tree.view</field>
			<field name="model">account.move.line</field>
			<field name="inherit_id" ref="account.view_move_line_tree_grouped_partner"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='name']" position="after">
					<field name="parnter_city" invisible="1"/>
                    <field name="parnter_ntn"/>
                    <field name="parnter_cnic"/>
                    <field name="partner_detail"  invisible="1"/>
				</xpath>
			</field>
		</record>
	</data>
</odoo>