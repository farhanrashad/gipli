<odoo>
    <data>


        <record id='checks_object_search' model='ir.ui.view'>
            <field name='name'>checks.management.search</field>
            <field name='model'>check.management</field>
            <field name='arch' type='xml'>
                <search>
                    <field name="check_number" string="Check Number"/>
                    <field name="from_check_number" string="From Check Number"/>
                    <field name="reservation_id"/>
                    <field name="custoemr_payment_id"/>
                    <field name="property_id"/>
                    <field name="request_reservation_id"/>
                    <!--                    <field name="investor_id" string="Customer"/>-->
                    <field name="amount" string="Check Amount"/>
                    <field name="currency_id"/>
                    <field name="partner_id"/>
                    <field name="payment_method"/>
                    <field name="receipt_number"/>
                    <field name="from_check_number"/>
                    <field name="from_check_date"/>
                    <field name="reject_reason"/>
                    <field name="reject_date"/>
                    <field name="old_payment_id_id" readonly="1"/>
                    <field name="old_payment_id_url" readonly="1" widget="url"/>


                    <filter string="Deposited Checks" name="deposite_checks" domain="[('state','=', 'depoisted')]"/>

                    <!--<filter string="Collecte Today" domain="[('check_date', '&gt;=', datetime.datetime.now().strftime('%Y-%m-%d 00:00:00')),('check_date', '&lt;=', datetime.datetime.now().strftime('%Y-%m-%d 23:23:59'))]"/>-->
                    <!--<filter string="Collecte Future >= today" domain="[('check_date', '&gt;=', datetime.datetime.now())]"/>-->
                    <!--<filter string="Overdue Collection" domain="[('check_date', '&lt;=', time.strftime('%%Y-%%m-%%d'))]"/>-->
                    <!--<filter string="Overdue Collection" domain="[('check_date', '&lt;=', datetime.datetime.now())]"/>-->
                    <!--<separator/>-->
                    <!--<filter name="check_date" string="Check Date" context="{'group_by': 'check_date'}"/>-->
                    <!--<filter name="state" string="Check State" context="{'group_by': 'state'}"/>-->
                    <!--<filter name="investor_id" string="Customer" context="{'group_by': 'partner_id'}"/>-->
                    <!--<filter name="deposite_bank" string="Deposite Bank" context="{'group_by': 'dep_bank'}"/>-->


                </search>
            </field>
        </record>


        <record id="checks_object_form" model="ir.ui.view">
            <field name="name">checks.object.form</field>
            <field name="model">check.management</field>
            <!--            <field name="view_type">form</field>-->
            <field name="arch" type="xml">
                <form string="" create="false">
                    <header>
                        <field name="state" widget="statusbar"/>
                        <!--                        <button name="action_cancel" string="Cancel" type="object" class="oe_highlight"-->
                        <!--                                states="draft,holding,handed"/>-->
                        <!--                        <button name="action_reset_draft" string="Reset To Draft" type="object" class="oe_highlight"-->
                        <!--                                states="draft,holding,handed"/>-->
                        <button name="%(check_split_action)d" type="action"
                                context="{'default_check_id':active_id,'default_amount_paid':amount_paid,'default_amount_due':amount_due,'default_amount':amount}"
                                string="Split"
                                states="holding,send_to_lagel,returned"
                                class="oe_highlight"/>
                    </header>
                    <sheet>
                        <div class="oe_button_box" name="button_box">

                            <button name="button_journal_entries" type="object" class="oe_stat_button"
                                    icon="fa-bars">
                                <div class="o_stat_info">
                                    <span class="o_stat_text">Journal Entry</span>
                                </div>
                            </button>
                        </div>
                        <group>
                            <group string="Check Info">
                                <field name="type" readonly="1" invisible="1"/>
                                <field name="check_number" readonly="1"/>
                                <field name="check_bank"/>
                                <field name="check_date" readonly="1"/>
                                <field name="dep_bank"/>
                                <field name="amount" readonly="1"/>
                                <field name="open_amount" readonly="1"/>
                                <field name="is_split" readonly="1"/>
                                <field name="amount_paid" readonly="1"/>
                                <field name="amount_due" readonly="1"/>
                                <field name="batch" readonly="1"/>
                                <!--<field name="will_collection" />-->
                                <field name="will_collection_user"/>
                                <field name="reservation_id"/>
                                <field name="custoemr_payment_id"/>
                                <field name="property_id"/>
                                <field name="request_reservation_id"/>
                                <field name="payment_method"/>
                                <field name="receipt_number"/>
                                <field name="customer_type_id"  options='{"no_open": True, "no_create": True}' attrs="{'readonly': [('state', '!=', 'draft')]}"/>
                                <field name="from_check_number" readonly="1"/>
                                <field name="from_check_date" readonly="1"/>

                            </group>

                            <group string="Customer Info">

                                <field name="partner_id" readonly="1" invisible="0"/>
                                <field name="deliver_to_customer_date" readonly="1"/>
                                <field name="reject_reason" readonly="1"/>
                                <field name="reject_date" readonly="1"/>
                            </group>

                        </group>

                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers" groups="base.group_user"/>
                        <field name="activity_ids" widget="mail_activity"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
                </form>
            </field>
        </record>


        <record id="checks_object_tree" model="ir.ui.view">
            <field name="name">checks.object.tree</field>
            <field name="model">check.management</field>
            <!--            <field name="view_type">tree</field>-->
            <field name="arch" type="xml">
                <tree string="" create="false" edit="false">
                    <field name="check_number"/>
                    <field name="check_date"/>
                    <field name="check_payment_id"/>
                    <field name="payment_date"/>
                    <field name="investor_id"/>
                    <field name="check_bank"/>
                    <field name="dep_bank"/>
                    <field name="amount" sum="Total Amount"/>
                    <field name="open_amount" sum="Total Open Amount"/>
                    <field name="type" invisible="1"/>
                      <field name="deliver_to_customer_date"/>
                      <field name="reject_reason"/>
                    <field name="reject_date"/>
                    <field name="batch"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>

