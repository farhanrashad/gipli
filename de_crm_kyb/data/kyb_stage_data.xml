<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record model="crm.stage" id="stage_kyb_new">
        <field name="name">New</field>
        <field name="is_kyb">True</field>
        <field name="sequence">100</field>
    </record>
    <record model="crm.stage" id="stage_kyb_progress">
        <field name="name">In Progress</field>
        <field name="is_kyb">True</field>
        <field name="sequence">110</field>
    </record>
    <record model="crm.stage" id="stage_kyb_verified">
        <field name="name">Verified</field>
        <field name="fold" eval="False"/>
        <field name="is_won">True</field>
        <field name="is_kyb">True</field>
        <field name="sequence">120</field>
    </record>
    <record model="crm.stage" id="stage_kyb_unverified">
        <field name="name">Unverified</field>
        <field name="fold" eval="False"/>
        <field name="is_kyb">True</field>
        <field name="sequence">130</field>
    </record>
</odoo>