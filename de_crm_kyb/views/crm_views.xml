<odoo>
    <menuitem
        id="menu_crm_kyb_opportunities"
        name="KYB Pipeline"
        parent="crm.crm_menu_sales"
        action="de_crm_kyb.action_my_kyb_pipeline"
        sequence="1"/>


      <record model="ir.actions.act_window" id="action_crm_kyb_pipeline">
            <field name="name">KYB Pipeline</field>
            <field name="res_model">crm.lead</field>
            <field name="view_mode">kanban,tree,graph,pivot,form,calendar,activity</field>
            <field name="domain">[('type','=','opportunity'),('is_kyb','=',True)]</field>
            <field name="context">{
                    'default_type': 'opportunity',
                    'default_is_kyb': 1,
                    'search_default_assigned_to_me': 1
            }</field>
            <field name="search_view_id" ref="crm.view_crm_case_opportunities_filter"/>
        </record>
      
</odoo>
