<?xml version="1.0"?>
<odoo>

    <record id="hr_contract_view_form_int" model="ir.ui.view">
        <field name="name">hr.contract.form.int</field>
        <field name="model">hr.contract</field>
        <field name="inherit_id" ref="hr_contract.hr_contract_view_form"/>
        <field name="arch" type="xml">

            <xpath expr="//field[@name='wage_type']"
                   position="before">
                <field name="transportation_amount"/>
            </xpath>

        </field>
    </record>
</odoo>
