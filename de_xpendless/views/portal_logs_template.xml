<!-- your_module/views/portal_xpendless_logs_template.xml -->
<odoo>
    <template id="portal_xpendless_logs">
        <t t-call="portal.portal_layout">
            <div class="container">
                <h1>Xpendless Logs</h1>
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th>Webhook Log ID</th>
                            <th>Event Name</th>
                            <th>Status</th>
                            <th>Type</th>
                            <th>Endpoint</th>
                            <th>Max Retries</th>
                            <th>Retries</th>
                            <th>Infinity Retries</th>
                            <th>Retry Interval</th>
                            <th>Created At</th>
                            <th>Updated At</th>
                        </tr>
                    </thead>
                    <tbody>
                        <t t-foreach="logs" t-as="log">
                            <tr>
                                <td><t t-esc="log['webhookLogId']"/></td>
                                <td><t t-esc="log['eventName']"/></td>
                                <td><t t-esc="log['status']"/></td>
                                <td><t t-esc="log['type']"/></td>
                                <td><t t-esc="log['endpoint']"/></td>
                                <td><t t-esc="log['maxRetries']"/></td>
                                <td><t t-esc="log['retries']"/></td>
                                <td><t t-esc="log['isInfinityRetries']"/></td>
                                <td><t t-esc="log['retryInterval']"/></td>
                                <td><t t-esc="log['createdAt']"/></td>
                                <td><t t-esc="log['updatedAt']"/></td>
                            </tr>
                        </t>
                    </tbody>
                </table>
            </div>
        </t>
    </template>
</odoo>
